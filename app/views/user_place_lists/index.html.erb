<p id="notice"><%= notice %></p>

<h1>Listing User Place Lists</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_place_lists.each do |user_place_list| %>
      <tr>
        <td><%= link_to 'Show', user_place_list %></td>
        <td><%= link_to 'Edit', edit_user_place_list_path(user_place_list) %></td>
        <td><%= link_to 'Destroy', user_place_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User place list', new_user_place_list_path %>
